{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "module": "commonjs",
    "noEmit": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "target": "es6"
  },
  "exclude": ["node_modules"],
  "paths": {
    "baseUrl": ".",
    "@typesafejs/shared/*": ["packages/shared/*"]
  }
}